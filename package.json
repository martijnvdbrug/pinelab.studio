{
  "name": "@mistergreen/pinelab-studio",
  "version": "1.0.0",
  "description": "",
  "main": "ts/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "npm run copy && concurrently \"npm run sass\" \"tsc -w\" \"npx @11ty/eleventy --serve\"  \"nodemon --watch dist --exec npm run browserify\"",
    "build": "npx @11ty/eleventy",
    "copy": "cp node_modules/foundation-sites/dist/js/foundation.min.js js/",
    "sass": "node-sass scss -o css --output-style compressed --watch",
    "browserify": "browserify dist/index.js | uglifyjs > js/bundle.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "browserify": "^16.5.1",
    "concurrently": "^5.2.0",
    "foundation-sites": "^6.6.3",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "typescript": "^3.9.6",
    "uglify-js": "^3.10.0"
  },
  "dependencies": {
    "three": "^0.118.3"
  }
}
