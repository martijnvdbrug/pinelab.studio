{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["es2016", "dom", "es5"],
    "sourceMap": false,
    "outDir": "dist",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ],
  "forceConsitentCasingInFileNames": true,
  "noImplicitReturns": true,
  "strict": true,
  "noUnusedLocals": true
}
